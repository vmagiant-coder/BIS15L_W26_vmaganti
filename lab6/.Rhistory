library("tidyverse")
install.packages("janitor")
library("janitor")
gabon <- read_csv("data/IvindoData_DryadVersion.csv")
summary(gabon)
glimpse(gabon)
gabon <- gabon %>%
clean_names()
glimpse(gabon)
new_gabon <- gabon %>%
mutate(across(c(hunt_cat, land_use, transect_id), as.factor))
#select id and land use and logging and see how many times it shows up -> bar graph
new_gabon %>%
select(transect_id, land_use)
distinct() %>%
ggplot(data = new_gabon,
mapping=aes(x=land_use)) +
geom_bar(mapping=aes(color=land_use)) +
labs(title = "Number of Transects by Land Use Type", x = "Land Use Type", y = "Number of Transects")
distinct() %>%
ggplot(data = new_gabon,
mapping=aes(x=land_use)) +
geom_bar(mapping=aes(fill=land_use)) +
labs(title = "Number of Transects by Land Use Type", x = "Land Use Type", y = "Number of Transects")
ggplot(data=new_gabon,
mapping=aes(x=land_use, y=veg_rich)) +
geom_boxplot() +
labs(title = "Vegetation Richness Across Land Use Types", x= "Land Use Type", y = "Vegetation Richness"")
ggplot(data=new_gabon,
mapping=aes(x=land_use, y=veg_rich)) +
geom_boxplot() +
labs(title = "Vegetation Richness Across Land Use Types", x= "Land Use Type", y = "Vegetation Richness")
ggplot(data=new_gabon,
mapping=aes(x=land_use, y=veg_rich)) +
geom_boxplot(aes(fill=land_use)) +
labs(title = "Vegetation Richness Across Land Use Types", x= "Land Use Type", y = "Vegetation Richness")
ggplot(data = new_gabon,
mapping=aes(x=veg_rich, y=rich_mammal_species)) +
geom_point() +
geom_smooth(method=lm, se=F) +
labs(title="Relationship Between Vegetation and Mammal Richness", x="Vegetation Richness", y="Mammal Richness")
names(new_gabon)
new_gabon %>%
filter(veg_rich > 10, rich_mammal_species > 10) %>%
select(transect_id, land_use, rich_mammal_species, veg_rich) %>%
arrange(desc(veg_rich))
new_gabon %>%
ggplot(data=new_gabon,
mapping=aes(x= land_use, y= diversity_all_species)) +
geom_boxplot() +
labs(title = "All Species Diversity Across Varying Land Types", x="Land Use Type", y="All Species Diversity")
new_gabon %>%
ggplot(data=new_gabon,
mapping=aes(x= land_use, y= diversity_all_species)) +
geom_boxplot(aes(fill=land_use)) +
labs(title = "All Species Diversity Across Varying Land Types", x="Land Use Type", y="All Species Diversity")
